#include <iostream>
using namespace std;

int main() {
    int a, b, c;

    cout << "Enter three integers: ";
    cin >> a >> b >> c;

    // Determine the largest number using if-else statements
    if (a >= b && a >= c) {
        cout << a << " is the largest number." << endl;
    } else if (b >= a && b >= c) {
        cout << b << " is the largest number." << endl;
    } else {
        cout << c << " is the largest number." << endl;
    }

    return 0;
}
